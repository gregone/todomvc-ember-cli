<section id='todoapp'>
  <header id='header'>
    <h1>todos</h1>
    {{todo-form}}
  </header>
  {{outlet}}
  <footer id='footer'>
    <span id='todo-count'><strong>2</strong> items left</span>
    <ul id='filters'>
      <li>{{#link-to 'todos.index'}}All{{/link-to}}</li>
      <li>{{#link-to 'todos.active'}}Active{{/link-to}}</li>
      <li>{{#link-to 'todos.completed'}}Completed{{/link-to}}</li>
    </ul>
    <button id='clear-completed'>Clear Completed (1)</button>
  </footer>
</section>
<footer id='info'>

</footer>
